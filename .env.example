# Basic Settings
APP_PORT=8080
APP_WORKERS=2
OCR_LANG=en
OCR_USE_GPU=false
MAX_UPLOAD_MB=12
MAX_PAGES=2
MIN_FOCUS=80
MAX_GLARE=0.08
REJECT_IF_BAD_QUALITY=true
DEBUG=false

# PP-Structure Settings
USE_PP_STRUCTURE=true
TABLE_STRUCTURE_MODEL=en

# Handwriting Detection Settings
ENABLE_HANDWRITING_DETECTION=true
HANDWRITING_THRESHOLD=0.6
TROCR_MODEL=microsoft/trocr-base-handwritten
